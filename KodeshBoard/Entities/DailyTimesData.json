{
  "name": "DailyTimesData",
  "type": "object",
  "properties": {
    "date": {
      "type": "string",
      "format": "date",
      "description": "Gregorian date"
    },
    "weekday": {
      "type": "number",
      "description": "Day of week (0-6)"
    },
    "hebrew_day": {
      "type": "string",
      "description": "Hebrew day name"
    },
    "hebrew_day_number": {
      "type": "string",
      "description": "Hebrew day number"
    },
    "hebrew_month": {
      "type": "string",
      "description": "Hebrew month name"
    },
    "is_special_day": {
      "type": "boolean",
      "description": "Whether this is a special day"
    },
    "special_day_type": {
      "type": "string",
      "description": "Type of special day if applicable"
    },
    "times": {
      "type": "object",
      "description": "Various time points for the day"
    }
  },
  "required": [
    "date"
  ]
}